<% @users.each do |entry|  %>
	<div class="row user_entry">
		<div class="col-md-3">
		<% if entry.image_url %>
			<img src="../images/<%= entry.image_url %>" alt="avatar">
		<% else %>
			<img src="../images/dummy_avatar.png" alt="">
		<% end %>
		</div>

		<div class="col-md-5">
			<ul class="user_entry_description">
				<li>Username: <%= entry.user_name %></li>
				<li>Email: <%= entry.email %></li>
				<li><%= link_to "details", "/users/#{entry.id}" %></li>
			</ul>
		</div>
	</div>
<% end %>